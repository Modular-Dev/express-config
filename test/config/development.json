{
  "domain": "http://localhost:3000",
  "port": 6001,
  "compressionThreshold": 512,
  "host": "localhost",
  "static": {
    "folderMount": "/assets",
    "folders": ["public"]
  },
  "maxAge": "1d",
  "development": true,
  "env": "development",
  "api": {
    "mount": "/api"
  },
  "view": {
    "engine": "ejs",
    "templateRoot": "templates",
    "templateLayouts": "templates/layouts/layout",
    "cache": true
  }
}